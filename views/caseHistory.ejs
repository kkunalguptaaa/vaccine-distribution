<%-include('msg');%> 
<div class="container">
    <div class="d-flex justify-content-between">
      <h2>Case History</h2>     
    </div>  
    <table class="table table-striped">
      <thead>
        <tr>
          <th>Mild Cases</th>
          <th>Serious Cases</th> 
          <th>Date and Time</th>
        </tr>
      </thead>
      <tbody>
          <%if(caseHistory===null||caseHistory.lenghth===0){%>
            <h5>No case history!<h5>
          <%}else{%>
            <%caseHistory.forEach(function(element){%>
            <tr>
                <td><%=element.mildCaseHistory%></td>
                <td><%=element.seriousCaseHistory%></td>
                <td><%=element.date%></td>
            </tr>
           <% })%>
        <%}%>
      </tbody>
    </table>
  </div>
